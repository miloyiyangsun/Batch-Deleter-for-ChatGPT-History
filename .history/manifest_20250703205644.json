{
    "manifest_version": 3,
    "name": "ChatGPT Finder",
    "version": "1.2",
    "description": "Deletes a specified number of conversations from chatgpt.com.",
    "permissions": ["scripting", "storage"],
    "host_permissions": [
        "https://chatgpt.com/*"
    ],
    "action": {
        "default_popup": "popup.html"
    }
} 